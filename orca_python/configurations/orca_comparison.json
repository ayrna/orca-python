{
    "general_conf": {
        "basedir": "orca_python/datasets/data",
        "datasets": [
            "balance-scale",
            "car",
            "contact-lenses",
            "ERA",
            "tae",
            "winequality-red"
        ],
        "hyperparam_cv_nfolds": 5,
        "jobs": -1,
        "input_preprocessing": "std",
        "output_folder": "my_runs/",
        "metrics": ["ccr", "mae", "mze"],
        "cv_metric": "mae"
    },
    "configurations": {
        "redsvm": {
            "classifier": "REDSVM",
            "parameters": {
                "C": [0.001, 0.01, 0.1, 1, 10, 100, 1000],
                "gamma": [0.001, 0.01, 0.1, 1, 10, 100, 1000]
            }
        },
        "svorex": {
            "classifier": "SVOREX",
            "parameters": {
                "C": [0.001, 0.01, 0.1, 1, 10, 100, 1000],
                "kappa": [0.001, 0.01, 0.1, 1, 10, 100, 1000]
            }
        }
    }
}
