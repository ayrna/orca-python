{
    "general_conf": {
        "basedir": "orca_python/datasets/data",
        "datasets": ["balance-scale"],
        "hyperparam_cv_nfolds": 3,
        "jobs": 10,
        "input_preprocessing": "std",
        "output_folder": "my_runs/",
        "metrics": ["ccr", "mae", "mze"],
        "cv_metric": "mae"
    },
    "configurations": {
        "svorex": {
            "classifier": "SVOREX",
            "parameters": {
                "kernel": "gaussian",
                "C": [0.001, 0.01, 0.1, 1, 10, 100, 1000],
                "kappa": [0.001, 0.01, 0.1, 1, 10, 100, 1000]
            }
        }
    }
}
